  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pYear">
            <Value>=Parameters!pYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pSalesperson">
            <Value>=Parameters!pSalesperson.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT fs.[City Key], dt.[Calendar Year] AS DeliveryYear,  cs.Customer AS CustomerName, [Primary Contact] AS PrimaryContact,  ct.City, ct.[State Province], es.Employee, sum(fs.[Total Excluding Tax]) AS 'Total Excluding tax'
FROM Dimension.city ct
           INNER JOIN Fact.Sale fs on (ct.[City Key] = fs.[City Key])
		   INNER JOIN Dimension.Date dt on (dt.Date = fs.[Delivery Date Key])
		   INNER JOIN Dimension.Customer cs on (fs.[Bill To Customer Key] = cs.[Customer Key])
		   INNER JOIN Dimension.Employee es on (fs.[Salesperson Key] = es.[Employee Key])
WHERE dt.[Calendar Year]  in (@pYear) AND es.Employee in (@pSalesperson)
GROUP BY fs.[City Key], dt.[Calendar Year],  cs.Customer, [Primary Contact],  ct.City, ct.[State Province], es.Employee
ORDER BY cs.Customer, ct.City, ct.[State Province], dt.[Calendar Year], es.Employee</CommandText>
      </Query>
