  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT fs.[City Key], dt.[Calendar Year] AS DeliveryYear,  cs.Customer AS CustomerName, [Primary Contact] AS PrimaryContact,  ct.City, ct.[State Province], es.Employee, sum(fs.[Total Excluding Tax]) AS 'Total Excluding tax'
FROM Dimension.city ct
           INNER JOIN Fact.Sale fs on (ct.[City Key] = fs.[City Key])
		   INNER JOIN Dimension.Date dt on (dt.Date = fs.[Delivery Date Key])
		   INNER JOIN Dimension.Customer cs on (fs.[Bill To Customer Key] = cs.[Customer Key])
		   INNER JOIN Dimension.Employee es on (fs.[Salesperson Key] = es.[Employee Key])
GROUP BY fs.[City Key], dt.[Calendar Year],  cs.Customer, [Primary Contact],  ct.City, ct.[State Province], es.Employee
ORDER BY cs.Customer, ct.City, ct.[State Province], dt.[Calendar Year], es.Employee</CommandText>
      </Query>
      <Fields>
        <Field Name="City_Key">
          <DataField>City Key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DeliveryYear">
          <DataField>DeliveryYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CustomerName">
          <DataField>CustomerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PrimaryContact">
          <DataField>PrimaryContact</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="State_Province">
          <DataField>State Province</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Employee">
          <DataField>Employee</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Excluding_tax">
          <DataField>Total Excluding tax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CityState">
          <Value>=Fields!City.Value + ", " + Fields!State_Province.Value</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
